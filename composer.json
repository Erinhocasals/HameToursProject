{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "bensampo/laravel-enum": "^3.4",
        "cviebrock/eloquent-sluggable": "^8.0",
        "doctrine/dbal": "^3.1",
        "fakerphp/faker": "^1.16",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/database": "^8.63",
        "jorenvanhocht/laravel-share": "^4.1",
        "josiasmontag/laravel-recaptchav3": "^0.6.0",
        "khsing/world": "^8.1",
        "laravel-lang/lang": "~8.0",
        "laravel/framework": "^8.54",
        "laravel/jetstream": "^2.4",
        "laravel/sanctum": "^2.6",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.5",
        "mews/captcha": "^3.2",
        "rosell-dk/webp-convert": "^2.6",
        "spatie/laravel-permission": "^5.1",
        "spatie/laravel-ray": "^1.26",
        "stevebauman/location": "^6.2",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "appzcoder/crud-generator": "^3.2",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "risul\\LaravelLikeComment\\": "packages/risul/laravel-like-comment/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
